{
  "dashboard": {
    "id": null,
    "title": "NextReel Application Monitoring",
    "description": "Comprehensive monitoring dashboard for NextReel movie recommendation service",
    "tags": ["nextreel", "application", "monitoring"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 39,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (req/min)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(nextreel_http_requests_total[1m])) * 60",
            "legendFormat": "Requests/min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 2,
        "title": "Response Time (P95)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(nextreel_http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 2.0}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate (%)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(sum(rate(nextreel_http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(nextreel_http_requests_total[5m]))) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "nextreel_active_users",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "HTTP Requests by Endpoint",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(nextreel_http_requests_total[1m])) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Database Connections",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "nextreel_db_connections_active",
            "legendFormat": "Active",
            "refId": "A"
          },
          {
            "expr": "nextreel_db_connections_idle",
            "legendFormat": "Idle",
            "refId": "B"
          },
          {
            "expr": "nextreel_db_connections_total",
            "legendFormat": "Total",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "Response Time Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(nextreel_http_request_duration_seconds_bucket[1m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Database Query Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(nextreel_db_query_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "P50 - {{query_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(nextreel_db_query_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "P95 - {{query_type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(nextreel_db_query_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "P99 - {{query_type}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "nextreel_db_circuit_breaker_state",
            "legendFormat": "Circuit Breaker",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "red", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "CLOSED", "color": "green"}}, "type": "value"},
              {"options": {"1": {"text": "OPEN", "color": "red"}}, "type": "value"},
              {"options": {"2": {"text": "HALF-OPEN", "color": "yellow"}}, "type": "value"}
            ]
          }
        }
      },
      {
        "id": 10,
        "title": "Movie Recommendations/min",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(nextreel_movie_recommendations_total[1m])) * 60",
            "legendFormat": "Recommendations/min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 11,
        "title": "User Actions",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(nextreel_user_actions_total[1m])) by (action_type)",
            "legendFormat": "{{action_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        }
      },
      {
        "id": 12,
        "title": "TMDB API Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(nextreel_tmdb_api_calls_total[1m])) by (endpoint)",
            "legendFormat": "{{endpoint}} - Calls/min",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(nextreel_tmdb_api_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}} - P95 Duration",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 13,
        "title": "Application Errors",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "sum(rate(nextreel_application_errors_total[5m])) by (error_type, endpoint)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0.01}
              ]
            }
          }
        }
      }
    ]
  }
}